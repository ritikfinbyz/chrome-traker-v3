{
    "manifest_version": 3,
    "name": "URL Tracker- v3",
    "version": "1.0",
    "description": "Capture and store all visited URLs.",
    "permissions": [
      "webNavigation",
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "http://localhost:5000/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  